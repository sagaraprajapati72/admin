{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Next.js: Debug",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "args": [
          "--inspect",
          "./node_modules/next/dist/bin/next",
          "dev"
        ],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }